{
  "name": "kasir_seblak",
  "version": "1.0.0",
  "description": "An easiest way to use php in electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --no-asar --icon=icons/logo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . kasir-seblak --overwrite --no-asar --platform=win32 --arch=ia32 --icon=icons/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Kasir Seblak\" --verbose",
    "package-linux": "electron-packager . kasir-seblak --overwrite --no-asar --platform=linux --arch=x64 --icon=icons/logo.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/zakkutsu/cashier_app",
  "keywords": [],
  "author": "zakkutsu",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.0",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    
    "php-server-manager": "^1.3.3"
  }
}
